{
  "name": "thinkjs-application",
  "description": "application created by thinkjs",
  "version": "1.0.0",
  "scripts": {
    "start": "node development.js",
    "compile": "babel --no-babelrc src/ --presets think-node --out-dir app/",
    "lint": "eslint src/",
    "lint-fix": "eslint --fix src/",
    "fe": "npm run copy && cd www/static && react-app-rewired start",
    "fe-build": "npm run copy && cd www/static && react-app-rewired build && npm run copy-html",
    "fe-test": "npm run copy && cd www/static && react-app-rewired test --env=jsdom",
    "fe-eject": "npm run copy && cd www/static && react-app-rewired eject",
    "copy": "node -e \"const fs = require('fs');fs.createReadStream('config-overrides.js').pipe(fs.createWriteStream('www/static/config-overrides.js'));fs.createReadStream('package.json').pipe(fs.createWriteStream('www/static/package.json'));\"",
    "copy-html": "node -e \"const fs = require('fs');fs.createReadStream('build/index.html').pipe(fs.createWriteStream('../../view/index_index.html'));\""
  },
  "dependencies": {
    "antd": "^2.13.9",
    "antd-mobile": "^2.0.2",
    "history": "^4.7.2",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-redux": "^5.0.6",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "think-cache": "^1.0.0",
    "think-cache-file": "^1.0.8",
    "think-logger3": "^1.0.0",
    "think-model": "^1.0.0",
    "think-model-mysql": "^1.0.0",
    "think-session": "^1.0.0",
    "think-session-file": "^1.0.5",
    "think-view": "^1.0.0",
    "think-view-nunjucks": "^1.0.1",
    "thinkjs": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-plugin-import": "^1.6.2",
    "babel-preset-think-node": "^1.0.0",
    "eslint": "^4.2.0",
    "eslint-config-think": "^1.0.0",
    "node-notifier": "^5.0.2",
    "react-app-rewired": "^1.3.5",
    "react-scripts": "1.0.14",
    "think-babel": "^1.0.3",
    "think-inspect": "0.0.2",
    "think-watcher": "^3.0.0"
  },
  "repository": "",
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "readmeFilename": "README.md",
  "homepage": "http://mywebsite.com/static/build",
  "thinkjs": {
    "metadata": {
      "name": "fancyshop",
      "description": "A shop with gun",
      "author": "Simon"
    },
    "projectName": "fancyshop",
    "templateName": "think-template/react",
    "cacheTemplatePath": "/home/simon/.think-templates/react",
    "clone": false,
    "isMultiModule": false
  }
}
