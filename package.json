{
  "name": "thinkjs-application",
  "description": "application created by thinkjs",
  "version": "1.0.0",
  "scripts": {
    "start": "node ./development.js",
    "compile": "babel --no-babelrc src/ --presets think-node --out-dir app/",
    "lint": "eslint src/",
    "lint-fix": "eslint --fix src/",
    "fe": "npm run copy && cd www/static && react-app-rewired start",
    "fe-build": "npm run copy && cd www/static && react-app-rewired build && npm run copy-html",
    "fe-test": "npm run copy && cd www/static && react-app-rewired test --env=jsdom",
    "fe-eject": "npm run copy && cd www/static && react-app-rewired eject",
    "copy": "node -e \"const fs = require('fs');fs.createReadStream('config-overrides.js').pipe(fs.createWriteStream('www/static/config-overrides.js'));fs.createReadStream('package.json').pipe(fs.createWriteStream('www/static/package.json'));\"",
    "copy-html": "node -e \"const fs = require('fs');fs.createReadStream('build/index.html').pipe(fs.createWriteStream('../../view/index_index.html'));\""
  },
